        "http-status": "^1.0.1",
        "ie-shim": "^0.1.0",
        "joi": "^10.5.2",
        "material": "^0.1.1",
        "method-override": "^2.3.9",
        "mongoose": "^4.10.6",
        "morgan": "^1.8.2",
        "mutationobserver-shim": "^0.3.2",
        "preboot": "^5.0.0-rc.10",
        "reflect-metadata": "^0.1.10",
        "rxjs": "~5.4.3",
        "ts-helpers": "^1.1.1",
